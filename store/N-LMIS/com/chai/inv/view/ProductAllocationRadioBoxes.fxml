<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.GridPane?>

<GridPane xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.chai.inv.ChooseProductAllocationController" fx:id="x_GRID_PANE_ALLOC_TYPE">
   <children>
      <Text fill="#3f13d1" strokeType="OUTSIDE" strokeWidth="0.0" text="Choose Product Allocation Type" GridPane.columnSpan="2" GridPane.halignment="CENTER">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
         <GridPane.margin>
            <Insets left="10.0" right="10.0" top="10.0" />
         </GridPane.margin>
      </Text>
      <RadioButton accessibleText="Emergency" mnemonicParsing="false" text="Emergency Allocation" GridPane.rowIndex="1" fx:id="x_EMERGENCY">
         <GridPane.margin>
            <Insets left="10.0" />
         </GridPane.margin>
      </RadioButton>
      <RadioButton accessibleText="Out-Reach" layoutX="20.0" layoutY="107.0" mnemonicParsing="false" text="Out Reach Allocation" GridPane.rowIndex="2" fx:id="x_OUT_REACH">
         <GridPane.margin>
            <Insets left="10.0" />
         </GridPane.margin>
      </RadioButton>
      <RadioButton accessibleText="Weekly" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="Weekly Allocation" GridPane.rowIndex="3" fx:id="x_WEEKLY">
         <GridPane.margin>
            <Insets left="10.0" />
         </GridPane.margin>
      </RadioButton>
      <RadioButton accessibleText="Monthly" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="Monthly Allocation" GridPane.rowIndex="4" fx:id="x_MONTHLY">
         <GridPane.margin>
            <Insets left="10.0" />
         </GridPane.margin>
      </RadioButton>
      <Button mnemonicParsing="false" onAction="#handleOK" prefWidth="52.0" text="OK" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="5">
         <GridPane.margin>
            <Insets left="50.0" />
         </GridPane.margin>
      </Button>
      <Button mnemonicParsing="false" onAction="#handleCancel" text="Cancel" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.rowIndex="5">
         <GridPane.margin>
            <Insets right="10.0" />
         </GridPane.margin>
      </Button>
   </children>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
   </rowConstraints>
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints minWidth="10.0" prefWidth="100.0" />
   </columnConstraints>
</GridPane>
