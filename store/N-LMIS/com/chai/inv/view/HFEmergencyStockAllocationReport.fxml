<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane stylesheets="@DisabledComboBoxOpacity.css" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.chai.inv.HFEmergencyStkAllocController">
   <top>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <Label text="HF Emergency Stock Allocation">
               <font>
                  <Font name="System Bold" size="21.0" />
               </font>
               <VBox.margin>
                  <Insets left="552.0" />
               </VBox.margin>
            </Label>
         </children>
      </VBox>
   </top>
   <center>
      <VBox spacing="20.0" BorderPane.alignment="CENTER">
         <children>
            <GridPane prefHeight="47.0" fx:id="x_GRID_FILTER">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="296.0" minWidth="10.0" prefWidth="193.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="296.0" minWidth="10.0" prefWidth="172.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="501.0" minWidth="10.0" prefWidth="155.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="514.0" minWidth="10.0" prefWidth="177.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="537.0" minWidth="10.0" prefWidth="424.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <ComboBox editable="true" onAction="#onHFChange" promptText="Select Health Facility" snapToPixel="false" visibleRowCount="6" GridPane.rowIndex="1" GridPane.valignment="BASELINE" fx:id="x_HF_DRPDN">
                     <GridPane.margin>
                        <Insets left="10.0" />
                     </GridPane.margin>
                  </ComboBox>
                  <Label text="Health Facility:" GridPane.valignment="BOTTOM">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <GridPane.margin>
                        <Insets left="10.0" />
                     </GridPane.margin>
                  </Label>
                  <Button mnemonicParsing="false" onAction="#onViewReportAction" prefHeight="26.0" prefWidth="93.0" text="View Report" GridPane.columnIndex="4" GridPane.rowIndex="1" GridPane.valignment="BASELINE">
                     <GridPane.margin>
                        <Insets left="10.0" />
                     </GridPane.margin>
                  </Button>
                  <Label layoutX="20.0" layoutY="15.0" text="Month:" GridPane.columnIndex="3" GridPane.valignment="BOTTOM" fx:id="x_MONTH_LBL">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <ComboBox onAction="#onMonthChange" prefWidth="150.0" promptText="Select Month" visibleRowCount="6" GridPane.columnIndex="3" GridPane.rowIndex="1" GridPane.valignment="BASELINE" fx:id="x_MONTH_DRPDN" />
                  <Label text="Filter By:" GridPane.columnIndex="1" GridPane.valignment="BOTTOM" fx:id="x_DATE_TYPE_LBL">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <GridPane.margin>
                        <Insets />
                     </GridPane.margin>
                  </Label>
                  <ComboBox layoutX="20.0" layoutY="34.0" onAction="#onDateTypeChange" prefHeight="25.0" prefWidth="166.0" promptText="Select Date Type" visibleRowCount="6" GridPane.columnIndex="1" GridPane.rowIndex="1" GridPane.valignment="BASELINE" fx:id="x_DATE_TYPE_DRPDN" />
                  <Label text="Year" GridPane.columnIndex="2" GridPane.valignment="BOTTOM" fx:id="x_YEAR_LBL">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <ComboBox layoutX="201.0" layoutY="33.0" onAction="#onYearChange" prefHeight="25.0" prefWidth="144.0" promptText="Select Year" visibleRowCount="6" GridPane.columnIndex="2" GridPane.rowIndex="1" GridPane.valignment="BASELINE" fx:id="x_YEAER_DRPDN" />
               </children>
            </GridPane>
            <TableView fx:id="x_HF_EMERGENCY_ALLOC_REPORT_TABLE">
              <columns>
                <TableColumn minWidth="150.0" prefWidth="-1.0" style="-fx-alignment: center-left;" text="Health Facility" fx:id="x_HF_COLUMN" />
                <TableColumn minWidth="100.0" prefWidth="-1.0" style="-fx-alignment: center-left;" text="Product" fx:id="x_ITEM_COLUMN" />
                  <TableColumn minWidth="100.0" prefWidth="-1.0" style="-fx-alignment: center;" text="Emergency Allocation Quantity" fx:id="x_EMERGENCY_ALLC_QUANT_COL" />
                  <TableColumn minWidth="120.0" prefWidth="-1.0" style="-fx-alignment: center;" text="Allocation Date" fx:id="x_ALLOCATION_DATE_COL" />
                  <TableColumn minWidth="120.0" prefWidth="-1.0" text="LGA Stock Balance" visible="false" fx:id="x_LGA_STOCK_BAL_COLUMN" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
         <BorderPane.margin>
            <Insets top="10.0" />
         </BorderPane.margin>
      </VBox>
   </center>
   <bottom>
      <HBox prefHeight="62.0" prefWidth="1143.0" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <Button contentDisplay="TOP" mnemonicParsing="false" onAction="#handleHomeDashBoardBtn" prefHeight="48.0" prefWidth="62.0" text="Home">
               <graphic>
                  <ImageView fitHeight="18.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../../resources/icons/home.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
            <Button contentDisplay="TOP" mnemonicParsing="false" onAction="#handleBackToReportsDashboard" prefHeight="48.0" prefWidth="59.0" text="Back">
               <graphic>
                  <ImageView fitHeight="18.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../../resources/icons/Back.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
         </children>
         <padding>
            <Insets left="20.0" top="5.0" />
         </padding>
      </HBox>
   </bottom>
</BorderPane>
