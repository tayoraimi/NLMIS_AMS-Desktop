                        <Insets left="10.0" />
                     </GridPane.margin>
                  </ComboBox>
                  <Label layoutX="498.0" layoutY="12.0" text="Product:" GridPane.columnIndex="5" GridPane.valignment="BOTTOM" fx:id="x_PRODUCT_LBL">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <GridPane.margin>
                        <Insets left="10.0" />
                     </GridPane.margin>
                  </Label>
                  <ComboBox editable="true" onAction="#onReasonChange" prefHeight="25.0" prefWidth="160.0" promptText="Select Type" snapToPixel="false" visibleRowCount="4" GridPane.columnIndex="4" GridPane.rowIndex="1" GridPane.valignment="BASELINE" fx:id="x_REASON_TYPE_DRPDN">
                     <GridPane.margin>
                        <Insets left="10.0" />
                     </GridPane.margin>
                  </ComboBox>
                  <Label text="Reason:" GridPane.columnIndex="4" GridPane.valignment="BOTTOM" fx:id="x_TRAN_TYPE_LBL">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <GridPane.margin>
                        <Insets left="10.0" />
                     </GridPane.margin>
                  </Label>
               </children>
            </GridPane>
            <TableView fx:id="x_LGA_ADJUSTMENT_REPORT_TABLE">
              <columns>
                <TableColumn minWidth="100.0" prefWidth="-1.0" style="-fx-alignment: center-left;" text="Product" fx:id="x_ITEM_COL" />
                  <TableColumn minWidth="120.0" prefWidth="-1.0" style="-fx-alignment: center;" text="Quantity Adjusted" fx:id="x_QUANT_ADJUST_COL" />
                  <TableColumn minWidth="120.0" prefWidth="-1.0" style="-fx-alignment: center-left;" text="Adjustment Type" fx:id="x_ADJUSTMENT_TYPE_COL" />
                  <TableColumn minWidth="120.0" prefWidth="-1.0" text="Date" visible="false" fx:id="x_DATE_COL" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
         <BorderPane.margin>
            <Insets top="10.0" />
         </BorderPane.margin>
      </VBox>
   </center>
   <bottom>
      <HBox prefHeight="62.0" prefWidth="1143.0" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <Button contentDisplay="TOP" mnemonicParsing="false" onAction="#handleHomeDashBoardBtn" prefHeight="48.0" prefWidth="62.0" text="Home">
               <graphic>
                  <ImageView fitHeight="18.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../../resources/icons/home.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
            <Button contentDisplay="TOP" mnemonicParsing="false" onAction="#handleBackToReportsDashboard" prefHeight="48.0" prefWidth="59.0" text="Back" fx:id="x_BACK">
               <graphic>
                  <ImageView fitHeight="18.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../../resources/icons/Back.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
         </children>
         <padding>
            <Insets left="20.0" top="5.0" />
         </padding>
      </HBox>
   </bottom>
</BorderPane>
