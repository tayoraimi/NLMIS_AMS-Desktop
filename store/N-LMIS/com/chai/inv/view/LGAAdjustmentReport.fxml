<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane stylesheets="@DisabledComboBoxOpacity.css" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.chai.inv.LGAAdjustmentReportController">
   <top>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <Label text="LGA Stock Adjustment  Report">
               <font>
                  <Font name="System Bold" size="21.0" />
               </font>
               <VBox.margin>
                  <Insets left="538.0" />
               </VBox.margin>
            </Label>
         </children>
      </VBox>
   </top>
   <center>
      <VBox spacing="20.0" BorderPane.alignment="CENTER">
         <children>
            <GridPane prefHeight="47.0" prefWidth="825.0" fx:id="x_GRID_FILTER">
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="296.0" minWidth="10.0" prefWidth="149.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="352.0" minWidth="0.0" prefWidth="115.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="501.0" minWidth="0.0" prefWidth="120.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="514.0" minWidth="10.0" prefWidth="117.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="537.0" minWidth="10.0" prefWidth="193.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="537.0" minWidth="10.0" prefWidth="187.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="682.0" minWidth="10.0" prefWidth="270.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
               </rowConstraints>
               <children>
                  <DatePicker onAction="#onDateChange" promptText="Select Date" visible="false" GridPane.columnIndex="1" GridPane.rowIndex="1" GridPane.valignment="BASELINE" fx:id="x_DATE" />
                  <ComboBox editable="true" onAction="#onLGAChange" prefHeight="25.0" prefWidth="180.0" promptText="Select LGA" snapToPixel="false" visibleRowCount="4" GridPane.columnIndex="3" GridPane.rowIndex="1" GridPane.valignment="BASELINE" fx:id="x_LGA_DRPDN">
                     <GridPane.margin>
                        <Insets left="10.0" />
                     </GridPane.margin>
                  </ComboBox>
                  <Label text="LGA:" GridPane.columnIndex="3" GridPane.valignment="BOTTOM" fx:id="x_LGA_LBL">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <GridPane.margin>
                        <Insets left="10.0" />
                     </GridPane.margin>
                  </Label>
                  <Button mnemonicParsing="false" onAction="#onViewReportAction" prefHeight="26.0" prefWidth="93.0" text="View Report" GridPane.columnIndex="6" GridPane.rowIndex="1" GridPane.valignment="BASELINE">
                     <GridPane.margin>
                        <Insets left="10.0" />
                     </GridPane.margin>
                  </Button>
                  <Label layoutX="20.0" layoutY="15.0" text="Month:" GridPane.columnIndex="2" GridPane.valignment="BOTTOM" fx:id="x_MONTH_LBL">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <ComboBox onAction="#onMonthChange" prefHeight="25.0" prefWidth="119.0" promptText="Select Month" visibleRowCount="6" GridPane.columnIndex="2" GridPane.rowIndex="1" GridPane.valignment="BASELINE" fx:id="x_MONTH_DRPDN" />
                  <Label text="Filter By:" GridPane.valignment="BOTTOM" fx:id="x_DATE_TYPE_LBL">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <GridPane.margin>
                        <Insets />
                     </GridPane.margin>
                  </Label>
